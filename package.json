{
  "name": "abrush-ai-functions",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "func start",
    "build": "tsc && npm run copy-files",
    "copy-files": "powershell -Command \"Copy-Item -Path 'host.json' -Destination 'dist/' -Force; Copy-Item -Path '.funcignore' -Destination 'dist/' -Force; Copy-Item -Path 'src/functions/*/function.json' -Destination 'dist/functions/' -Force -Recurse\"",
    "start": "func start",
    "preview": "vite preview"
  },
  "dependencies": {
    "@azure/functions": "^4.9.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.19.25",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.8"
  }
}
